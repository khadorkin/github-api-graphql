{
  "name": "graphql-github-js",
  "version": "0.0.1",
  "description": "GraphQL overlay for GitHub",
  "options": {
    "mocha": "--compilers js:babel-register --require babel-polyfill src/**/test/**/*.js"
  },
  "scripts": {
    "build": "babel src -d lib",
    "check": "flow check",
    "lint": "eslint src",
    "start": "babel-node src/cloud/main.js",
    "update-schema": "babel-node ./scripts/updateSchema.js",
    "test": "npm run lint && npm run check && npm run testonly",
    "testonly": "mocha $npm_package_options_mocha"
  },
  "author": "",
  "license": "ISC",
  "homepage": "https://github.com/rportugal/graphql-github-js",
  "dependencies": {
    "babel-polyfill": "^6.9.1",
    "body-parser": "^1.15.0",
    "dataloader": "^1.2.0",
    "ejs": "^2.4.1",
    "express": "^4.13.3",
    "express-graphql": "^0.4.9",
    "express-partials": "^0.3.0",
    "express-session": "^1.13.0",
    "graphql": "^0.4.17",
    "method-override": "^2.3.5",
    "passport": "^0.3.2",
    "passport-github2": "^0.1.9",
    "react": "^0.14.5",
    "restler": "^3.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "eslint": "^3.1.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.11.1",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^5.2.2",
    "flow-bin": "^0.18.1",
    "mocha": "^2.2.5"
  },
  "engines": {
    "node": "6.x"
  }
}
