{
  "name": "graphql-github-js",
  "version": "0.1.0",
  "description": "GraphQL overlay for GitHub",
  "options": {
    "mocha": "--compilers js:babel-register --require babel-polyfill src/**/test/**/*.js"
  },
  "scripts": {
    "build": "babel src -d build",
    "check": "flow check",
    "lint": "eslint src",
    "start": "nodemon --exec babel-node -- src/server/main.js",
    "update-schema": "babel-node ./scripts/updateSchema.js",
    "test": "npm run lint && npm run check && npm run testonly",
    "testonly": "mocha $npm_package_options_mocha --watch"
  },
  "author": "",
  "license": "ISC",
  "homepage": "https://github.com/rportugal/graphql-github-js",
  "dependencies": {
    "babel-polyfill": "^6.9.1",
    "body-parser": "^1.15.0",
    "chai": "^3.5.0",
    "dataloader": "^1.2.0",
    "dotenv": "^2.0.0",
    "ejs": "^2.4.1",
    "express": "^4.13.3",
    "express-graphql": "^0.5.4",
    "express-partials": "^0.3.0",
    "express-session": "^1.13.0",
    "graphql": "^0.7.0",
    "isomorphic-fetch": "^2.2.1",
    "method-override": "^2.3.5",
    "passport": "^0.3.2",
    "passport-github2": "^0.1.9",
    "react": "^15.3.1",
    "request": "^2.74.0",
    "request-promise-native": "^1.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "dirty-chai": "^1.2.2",
    "eslint": "^3.1.1",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-import": "^1.11.1",
    "eslint-plugin-jsx-a11y": "^2.2.0",
    "eslint-plugin-react": "^6.2.0",
    "flow-bin": "^0.31.1",
    "mocha": "^3.0.2",
    "nock": "^8.0.0"
  },
  "engines": {
    "node": "6.x"
  }
}
